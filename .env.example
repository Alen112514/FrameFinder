# FrameFinder Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Django Settings
SECRET_KEY=django-insecure-replace-with-secure-key-for-production
DEBUG=True

# Google API Key for Gemini (Required)
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_API_KEY=your-google-gemini-api-key-here

# OpenAI API Key for Whisper API (Optional)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-api-key-here

# Whisper Configuration

USE_OPENAI_WHISPER=True

# Database (Optional - defaults to SQLite)
# DATABASE_URL=postgresql://user:password@localhost:5432/framefinder

# Production Settings (Optional)
# ALLOWED_HOSTS=your-domain.com,127.0.0.1,localhost
# CORS_ALLOWED_ORIGINS=https://your-domain.com